{
    "name": "ms-general",
    "version": "1.0.0",
    "description": "Microsserviço General para endereço e contato",
    "main": "index.js",
    "scripts": {
        "start": "nodemon ./dev/server",
        "debug": "nodemon --legacy-watch --inspect=0.0.0.0 ./dev/server",
        "lint": "eslint ./api --fix-dry-run",
        "test": "echo \"Error: no test specified\" && exit 1",
        "migrate": "sequelize db:migrate",
        "seed": "sequelize db:seed:all",
        "seed-create": "yarn sequelize seed:generate --name",
        "gen": "sequelize migration:create --name",
        "undo": "sequelize db:migrate:undo",
        "status": "sequelize db:migrate:status",
        "build": "sucrase ./dev -d ./prod --transforms imports",
        "prod": "pm2 start ecosystem.config.js --env production",
        "logs": "pm2 logs ms-general",
        "delete": "pm2 delete ms-general"
    },
    "repository": {
        "type": "git",
        "url": "git+git@github.com:vinialves008/example-express-grpc.git"
    },
    "author": "",
    "license": "ISC",
    "homepage": "https://github.com/vinialves008/example-express-grpc/blob/master/README.md",
    "dependencies": {
        "@grpc/grpc-js": "^1.3.7",
        "@grpc/proto-loader": "^0.1.0",
        "config": "^3.3.1",
        "cors": "^2.8.5",
        "cpf_cnpj": "^0.2.0",
        "dotenv": "^8.2.0",
        "ejs": "^3.1.5",
        "enum": "^3.0.4",
        "express": "^4.17.1",
        "grpc": "^1.24.11",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "multer": "^1.4.2",
        "multer-s3": "^2.9.0",
        "nodemailer": "^6.4.17",
        "pg": "^8.0.3",
        "pg-hstore": "^2.3.3",
        "pm2": "5.2.0",
        "sequelize": "6.17.0",
        "swagger-jsdoc": "6.1.0",
        "swagger-ui-express": "4.3.0",
        "witch": "^1.0.3",
        "xml-js": "^1.6.11",
        "yup": "^0.28.5"
    },
    "devDependencies": {
        "eslint": "^7.15.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^7.0.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.3.0",
        "nodemon": "^2.0.15",
        "prettier": "^2.2.1",
        "sequelize-cli": "^5.5.1",
        "sucrase": "^3.14.0"
    }
}